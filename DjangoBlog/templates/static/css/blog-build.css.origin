}

figure.hover_select.poke div.circle {
	width: 100px;
	height: 100px;
	border-radius: 100px;

	border: 10px solid #5b9819;
}

figure.hover_select p {
	display: block;
	line-height: 18px;
	font-family: 'freight-sans-pro', 'Microsoft YaHei', '文泉驿微米黑', sans-serif;
	font-weight: 900;
	margin: 0;
	margin-top: 1px;

	font-size: 20px;
	color: #000;
	text-align: center;

}

figure.hover_select p.identifier {
	font-size: 11px;
	font-weight: 800;
	letter-spacing: 1px;
	line-height: 11px;
}

figure.hover_select p.kcount {
		margin-bottom: 5px;
		margin-top: 4px;
}

figure.hover_select.done p.kcount {
	text-transform: uppercase;
	font-size: 15px;
	margin-top: 5px;
	letter-spacing: 1px;
}

figure.hover_select.poke p.kcount {
	color: #5b9819;
}

figure.hover_select.poke.done p.kcount {
	color: #000;
}

figure.hover_select p.kcount.active {
	font-size: 15px;
	margin-top: 10px;
}

figure.hover_select.done div.circle {
	background: #000 url(/images/brand/white_lightning.png) center center no-repeat;
	border: 4px solid #000;
}

figure.hover_select.poke.done div.circle {
	border: 10px solid #000;
}

figure.hover_select.done div.filled {
	display: none;
}

figure.hover_select div.filled {
	position: absolute;
	background-color: #000;
	width: 40px;
	height: 40px;
	-webkit-border-radius: 40px;
	-moz-border-radius: 40px;
	border-radius: 40px;
	top: 0px;
	-webkit-transform: scale(0.2);
	-moz-transform: scale(0.2);
}


figure.hover_select.poke div.filled {
	position: absolute;
	background-color: #5b9819;
	width: 100px;
	height: 100px;
	-webkit-border-radius:100px;
	-moz-border-radius: 100px;
	border-radius: 100px;
	top: 0px;
	-webkit-transform: scale(0.2);
	-moz-transform: scale(0.2);
}

figure.hover_select.animate div.filled {
	-webkit-animation-name: hoverPulse;
	-moz-animation-name: hoverPulse;
	-webkit-animation-duration: 3s;
	-moz-animation-duration: 3s;
	-webkit-animation-iteration-count: infinite;
	-moz-animation-iteration-count: infinite;
	-webkit-animation-timing-function:linear;
	-moz-animation-timing-function: linear;
}

figure.hover_select:hover div.filled {
  -webkit-animation-name: hoverSelect;
  -moz-animation-name: hoverSelect;
	-webkit-animation-duration: 1s;
	-moz-animation-duration: 1s;
	-webkit-animation-iteration-count: 1;
	-moz-animation-iteration-count: 1;
	-webkit-animation-timing-function:linear;
	-moz-animation-timing-function:linear;
}

