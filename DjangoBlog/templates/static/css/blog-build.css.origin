aside#network_blog {
	position: absolute;
	left: 0px;
	top: 0px;
	bottom: 0px;
	right: auto;
	background-color: #000;
	width: 0px;
	z-index: 99999;

	overflow: hidden;
	display: none;

	-webkit-transition-property: all;
	-webkit-transition-duration: 1s;
	-webkit-transition-timing-function: ease-in-out;

	-moz-transition-property: all;
	-moz-transition-duration: 1s;
	-moz-transition-timing-function: ease-in-out;
}

aside#network_blog.bigsite {
	position: fixed;
}

div.identify {
	display: none;
	margin-top: 100px;
	position: fixed;
	left: 60px;
	top: 0px;
	z-index: 999999;
	-webkit-transition-property: all;
	-webkit-transition-duration: 0.7s;
	-webkit-transition-timing-function: ease-in-out;

	-moz-transition-property: all;
	-moz-transition-duration: 1s;
	-moz-transition-timing-function: ease-in-out;
}

div.identify.die {
	-webkit-transform: scale(0.2);
	-moz-transform: scale(0.2);

	left: -200px;
	opacity: 0;
}

div.identify h3 {
	background-position: center center;
	background-repeat: no-repeat;

	border-radius: 50%;
	border: 10px solid #fff;

	width: 100px;
	height: 100px;
	margin: 0 auto;
}

div.identify p.name {
	font-weight: 800;
	text-align: center;
	display: block;
	color: #fff;
	font-size: 30px;
	padding-top: 50px;
}

aside#network_blog a {
	display: block;
	position: absolute;
	left: 0px;
	right: 0px;
	top: 0px;
	bottom: 0px;
}

aside#network_blog h4 {
	color: #fff;
	text-transform: uppercase;
	margin: 0px;
	padding: 0px;
	padding-top: 5px;

	width: 200px;

	-webkit-transform:rotate(-90deg);
	-moz-transform:rotate(-90deg);
	-o-transform:rotate(-90deg);

	-webkit-transform-origin: top left;
	-moz-transform-origin: top left;
	-o-transform-origin: top left;

	position: absolute;
	bottom: 0px;

}
