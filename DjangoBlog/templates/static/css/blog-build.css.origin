@-webkit-keyframes pulsationHover
	{
	0% {
		width: 8px;
		height: 8px;
		margin-top: -4px;
		border-radius: 8px;
		background-color: #000;
	}
	100% {
		width: 40px;
		height: 40px;
		margin-top: -20px;
		border-radius: 40px;
		background-color: #000;
	}
}

@-moz-keyframes pulsationHover
	{
	0% {
		width: 8px;
		height: 8px;
		margin-top: -4px;
		border-radius: 8px;
		background-color: #000;
	}
	100% {
